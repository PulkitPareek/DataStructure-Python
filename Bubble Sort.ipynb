    "def bubble_sort(arr):\n",
    "    \n",
    "    for x in range(len(arr)-1,0,-1):\n",
    "        for y in range(x):\n",
    "            if arr[y]>arr[y+1]:\n",
    "                tmp = arr[y]\n",
    "                arr[y] = arr[y+1]\n",
    "                arr[y+1] = tmp\n",
    "            "
   ]
